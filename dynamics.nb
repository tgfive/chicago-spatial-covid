    RowBox[{"\[Mu]", "*", "s"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eT", "=", 
   RowBox[{
    RowBox[{"\[Beta]SA", "*", "s", "*", "a"}], "+", 
    RowBox[{"\[Beta]SI", "*", "s", "*", "i"}], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[Sigma]A", "+", "\[Sigma]I"}], ")"}], "*", "e"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
